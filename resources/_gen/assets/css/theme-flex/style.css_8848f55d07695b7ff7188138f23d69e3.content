  padding: 0 10px 0 10px;
  width: 75%;
}

.home-options__option ul {
  font-size: 1.15rem;
  list-style-type: none;
  padding-left: 30%;
  text-align: left;
}

.home-options__option ul li {
  line-height: 1.5;
  padding-left: 2rem;
  position: relative;  
}

.home-options__option ul li a:hover {  
  color: #3d5051;
  text-decoration: none;
}

.home-options__option ul li::before {
  content: "";
  display: block;
  position: absolute;
  background: url(./img/ico-check-mark.svg);
  background-repeat: no-repeat;
  left: 0;
  top: .375rem;
  width: 1.175rem;
  height: 1.588rem;
}

@media screen and (max-width: 1024px){
  .home-options {
    width: 860px;
  }
}

@media screen and (max-width: 860px){
  .home-hero h1 {
    font-size: 2.5rem;
  }

  .home-hero p {
    font-size: 1.6rem;
    width: 98%;
  }

  .home-options {
    flex-direction: column;
    width: 100%;
  }

  .home-options__option {
    margin-bottom: 40px;
  }

  .home-options__option p {
    width: 88%;
  }

  .home-options__option ul {
    padding-left: 25%;
  }
}

@media screen and (min-width: 450px) and (max-width: 860px){
  .home-options__option p {
    width: 50%;
  }

  .home-options__option ul {
    padding-left: 40%;
  }
}

